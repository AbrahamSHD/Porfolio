---
import { ICONS } from '../utils/icons'

// const { class: className } = Astro.props
const iconEntries = Object.entries(ICONS);
---

{ 
    iconEntries.map(([key, tag]) => (
        <picture class="flex justify-center items-center bg-slate-300/50 size-14 max-w-16 max-h-16 rounded">
            <tag.icon 
                class={ tag.skillClass }
            />
        </picture>
    ))
}
